import java.awt.*;
import java.util.*;

public class TurtleTestGetSet
{
    public static void main(String[] args)
    {
        World world = new World(300, 300);
        Turtle yertle = new Turtle(world);
        System.out.println(
                "Yertle is starting at: "
                        + yertle.getXPos()
                        + ", "
                        + yertle.getYPos());
        yertle.forward(100);
        yertle.turn(90);
        yertle.forward(50);
        System.out.println(
                "Yertle has ended up at: "
                        + yertle.getXPos()
                        + ", "
                        + yertle.getYPos());
        world.show(true);
    }
}

