// comments?
public class User
{

    private String username;
    private String password;

    public User()
    {
        username = "guest";
        password = "guest" + (int) (Math.random() * 1000);
    }

    public User(String nameInit, String pwordInit)
    {
        username = nameInit;
        password = pwordInit;
    }

    public void welcome()
    {
