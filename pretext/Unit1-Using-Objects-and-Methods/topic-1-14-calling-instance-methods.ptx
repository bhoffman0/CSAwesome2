    <exercise label="TurtleDraw8">
      <statement>
        <p>Can you make yertle draw the digital number 8, as 2 squares on top of each other?</p>
      </statement>
      <program xml:id="UNIQUE-ID-000053" interactive="activecode" language="java">
        <input>
import java.awt.*;
import java.util.*;

public class TurtleDraw8
{
    public static void main(String[] args)
    {
        World habitat = new World(500, 500);
        Turtle yertle = new Turtle(habitat);
        // Make yertle draw an 8 with 2 squares
        yertle.forward();

        habitat.show(true);
    }
}

