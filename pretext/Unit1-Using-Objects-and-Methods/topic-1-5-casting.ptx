        if (actual1.length() &gt; 0 || actual2.length() &gt; 0)
        {
            actual = (actual1 + "\n" + actual2);
        }

        if (!passed)
        {
            hint = "\n(Check spelling and capitalization)";
        }

        getResults(
                expect,
                actual,
                "Checking that grades have been added together and divided by"
