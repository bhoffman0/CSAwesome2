import java.util.*;

public class TestForLoop
{
    public static void main(String[] args)
    {
        ArrayList&lt;Integer&gt; myList = new ArrayList&lt;Integer&gt;();
        myList.add(50);
        myList.add(30);
        myList.add(20);
        int total = 0;
        for (int i = 0; i &lt;= myList.size(); i++)
        {
            total = total + myList.get(i);
