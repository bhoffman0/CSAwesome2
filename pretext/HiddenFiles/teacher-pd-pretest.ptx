</statement><feedback id="PDpretest4_opt_b">
</feedback></choice><choice><statement id="PDpretest4_opt_c">            <p>liquid.lowerTemp();</p>
</statement><feedback id="PDpretest4_opt_c">
</feedback></choice><choice correct="yes"><statement id="PDpretest4_opt_d">            <p>liquid.raiseTemp(10);</p>
</statement><feedback id="PDpretest4_opt_d">
</feedback></choice><choice><statement id="PDpretest4_opt_e">            <p>liquid.freeze(10)</p>
</statement><feedback id="PDpretest4_opt_e">
